{
    "name": "digedag/t3twigs",
    "description": "Use twig templates for TYPO3",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "twig",
        "template"
    ],
	"homepage": "http://www.system25.de/",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Ren√© Nitzsche",
            "email": "rene@system25.de",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7 || ^9.5 || ^10.4 || ^11.5 || ^12.4",
        "digedag/rn-base": "~1.18.0",
        "twig/twig": "~2.16"
    },
    "suggest": {
        "twig/extensions": "Common additional features for Twig that do not directly belong in core. See @Documentation/reference/extensions/ThirdParty.md"
    },
    "autoload": {
        "psr-4": {
            "System25\\T3twigs\\": "Classes"
        }
    },
    "support": {
        "email": "rene@system25.de"
    },
    "replace": {
        "typo3-ter/t3twigs": "*"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "preferred-install": {
            "typo3/cms": "source"
        }
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
